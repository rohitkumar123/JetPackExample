<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jetpackexample">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SampleActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".observer.MainActivity"/>
        <activity android:name=".viewbinding.ViewBindingActivity"/>
        <activity android:name=".databinding.activity.DataBindingActivity"/>
        <activity android:name=".databinding.activity.SecondActivity"/>
        <activity android:name=".databinding.activity.TwoWayDataBinding"/>
    </application>

</manifest>